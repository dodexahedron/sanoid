{
  "$schema": "https://raw.githubusercontent.com/dodexahedron/sanoid/dotnet-port-master/dotnet/Sanoid/Sanoid.schema.json",
  "$id": "https://raw.githubusercontent.com/dodexahedron/sanoid/dotnet-port-master/dotnet/Sanoid/Sanoid.json",
  "UseSanoidConfiguration": true,
  "SanoidConfigurationPathBase": "/etc/sanoid",
  "SanoidConfigurationDefaultsFile": "sanoid.defaults.conf",
  "SanoidConfigurationLocalFile": "sanoid.conf",
  "Formatting": {
    "SnapshotNaming": {
      "ComponentSeparator": "_",
      "Prefix": "autosnap",
      "TimestampFormatString": "yyyy-MM-dd_HH\\:mm\\:ss",
      "FrequentSuffix": "frequently",
      "HourlySuffix": "hourly",
      "DailySuffix": "daily",
      "WeeklySuffix": "weekly",
      "MonthlySuffix": "monthly",
      "YearlySuffix": "yearly"
    }
  }
}